{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://eco-commerce-2vxl.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm install && npx vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install"
} 